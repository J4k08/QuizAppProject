<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jakobhaglof.quizapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/standardmonkey"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProfileActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameSettingsActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".PostGameActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".HighScoreActivity" />
        <activity android:name=".AboutActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jakobhaglof.quizapp.SettingsActivity" />
        </activity>
        <activity android:name=".MainMenuActivity" />
        <activity android:name=".AddQuestionActivity" />
        <activity android:name=".RemoveQuestionActivity" />
        <activity android:name=".PersonalProfileActivity"/>
    </application>

</manifest>